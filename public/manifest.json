{
  "name": "Getting Started Example",
  "description": "Build an Extension!",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
      "default_popup": "index.html"
  },
  "permissions": ["activeTab", "scripting"],
  "background": {
    "service_worker": "./js/background.js"
  },
  "content_scripts": [{
    "matches": ["http://*/*", "https://*/*"],
    "js": ["./js/content.js"]
  }]
}